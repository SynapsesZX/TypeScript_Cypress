{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["cypress"],
    "baseUrl": ".",
    "paths": {
      "@fixtures/*": ["cypress/fixtures/*"]
    }
  },
  "include": [
    "cypress/e2e/**/*.ts",
    "cypress/support/**/*.ts",
    "cypress/fixtures/**/*.ts"
  ]
}
